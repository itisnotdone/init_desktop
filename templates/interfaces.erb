# interfaces(5) file used by ifup(8) and ifdown(8)
auto lo
iface lo inet loopback
  dns-nameservers <%= node['init_desktop']['network_config']['dns'] %>
  #dns-domain default.don

auto <%= node['init_desktop']['network_config']['gate_dev'] %>
iface <%= node['init_desktop']['network_config']['gate_dev'] %> inet dhcp

source /etc/network/interfaces.d/*.cfg

